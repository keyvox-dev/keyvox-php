{
    "name": "keyvox/keyvox-php",
    "description": "KeyVox PHP library",
    "keywords": [
        "keyvox",
        "blogging",
        "api"
    ],
    "homepage": "https://keyvox.dev",
    "autoload": {
        "psr-4": {
            "KeyVox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KeyVox\\": [
                "tests/"
            ]
        }
    },
    "authors": [
        {
            "name": "keyvox-dev",
            "email": "157120239+keyvox-dev@users.noreply.github.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^2.33",
        "symfony/var-dumper": "^7.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "guzzlehttp/guzzle": "^7.8"
    }
}
